{
  "name": "integer-dev-tools",
  "version": "1.0.0",
  "description": "@TODO",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . integer-dev-tools --overwrite --platform=win32 --arch=ia32 --icon=images/icon.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Integer Dev Tools\" --ignore=.idea --ignore=public/^.*",
    "package-linux": "electron-packager . integer-dev-tools --overwrite --platform=linux --arch=x64 --icon=images/png/1024x1024.png --prune=true --out=release-builds",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=images/mac/icon.icns --prune=true --out=release-builds"
  },
  "repository": "https://github.com/Darciro/in-dev-tools",
  "author": "Ricardo Carvalho",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^21.2.3",
    "electron-packager": "^17.1.1"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "bootstrap": "^5.2.2",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.6.1",
    "popper": "^1.0.1"
  }
}
